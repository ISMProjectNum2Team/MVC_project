<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="NHibernateDataProvider" namespace="Business">
  <class name="Event" table="Events" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="ID">
      <generator class="guid" />
    </id>
    <property name="Title" />
    <property name="Description" />
    <property name="Details" />
    <property name="Location" />
    <property name="DateFrom" />
    <property name="DateTo" />
  </class>
</hibernate-mapping>